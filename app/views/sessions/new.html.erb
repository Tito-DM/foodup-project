<h1>Log In</h1>
<%= form_for :session, url: login_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email, autofocus: true %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Log In",  class: 'btn green'%>
  </div>
<% end %>
